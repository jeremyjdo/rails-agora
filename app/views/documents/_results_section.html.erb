
  <% @documents_unselected.each do |document| %>
    <div class= "box">
      <%= link_to document_path(document) do %>
        <%= cl_image_tag document.photo, class: "document-thumbnail-large card-image", "format" => 'jpg' %>
      <% end %>
      <div class="card-description">
        <div class="card-title">
          <% if document.name[27] %>
            <h5><%= document.name[0..26] %>...</h5>
          <% else %>
            <h5><%= document.name %></h5>
          <% end %>
        </div>
        <div class="card-info">
          <p><%=document.updated_at.strftime("%Y-%m") %></p>
        </div>
      </div>
    </div>
  <% end %>



